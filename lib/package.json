{
  "name": "my-library",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./math": {
      "import": "./dist/esm/math.js",
      "require": "./dist/cjs/math.js",
      "types": "./dist/types/math.d.ts"
    },
    "./string": {
      "import": "./dist/esm/string.js",
      "require": "./dist/cjs/string.js",
      "types": "./dist/types/string.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "node esbuild.lib.config.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types --project tsconfig.json",
    "clean": "rm -rf dist",
    "build:swc": "npm run build:types && npm run build:swc:esm && npm run build:swc:cjs",
    "build:swc:esm": "swc src -d dist/esm --config-file swc.config.js",
    "build:swc:cjs": "swc src -d dist/cjs --config-file swc.config.js --config-json '{\"module\":{\"type\":\"commonjs\"}}'",
    "build:swc:min": "npm run build:types && npm run build:swc:esm:min && npm run build:swc:cjs:min",
    "build:swc:esm:min": "swc src -d dist/esm --config-file swc.config.js --config-json '{\"minify\":true}'",
    "build:swc:cjs:min": "swc src -d dist/cjs --config-file swc.config.js --config-json '{\"module\":{\"type\":\"commonjs\"},\"minify\":true}'"
  },
  "peerDependencies": {
    "react": "19.1.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.0",
    "esbuild": "0.25.12"
  },
  "dependencies": {
    "typescript": "5.9.3"
  }
}
